<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <inset>
            <scale android:scaleX="0.9" android:scaleY="0.9" android:duration="150"/>
        </inset>
    </item>
    <item>
        <inset>
            <scale android:scaleX="1.0" android:scaleY="1.0" android:duration="150"/>
        </inset>
    </item>
</selector>
